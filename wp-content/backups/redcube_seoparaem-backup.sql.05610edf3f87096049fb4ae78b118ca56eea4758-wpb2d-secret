--
-- Table structure for table `lig_prli_clicks`
--

CREATE TABLE `lig_prli_clicks` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ip` varchar(255) DEFAULT NULL,
  `browser` varchar(255) DEFAULT NULL,
  `btype` varchar(255) DEFAULT NULL,
  `bversion` varchar(255) DEFAULT NULL,
  `os` varchar(255) DEFAULT NULL,
  `referer` varchar(255) DEFAULT NULL,
  `host` varchar(255) DEFAULT NULL,
  `uri` varchar(255) DEFAULT NULL,
  `robot` tinyint(4) DEFAULT '0',
  `first_click` tinyint(4) DEFAULT '0',
  `created_at` datetime NOT NULL,
  `link_id` int(11) DEFAULT NULL,
  `vuid` varchar(25) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `link_id` (`link_id`),
  KEY `ip` (`ip`),
  KEY `browser` (`browser`),
  KEY `btype` (`btype`),
  KEY `bversion` (`bversion`),
  KEY `os` (`os`),
  KEY `referer` (`referer`),
  KEY `host` (`host`),
  KEY `uri` (`uri`),
  KEY `robot` (`robot`),
  KEY `first_click` (`first_click`),
  KEY `vuid` (`vuid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Table `lig_prli_clicks` is empty
--

--
-- Table structure for table `lig_prli_groups`
--

CREATE TABLE `lig_prli_groups` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `description` text,
  `created_at` datetime NOT NULL,
  PRIMARY KEY (`id`),
  KEY `name` (`name`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Table `lig_prli_groups` is empty
--

--
-- Table structure for table `lig_prli_link_metas`
--

CREATE TABLE `lig_prli_link_metas` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `meta_key` varchar(255) DEFAULT NULL,
  `meta_value` longtext,
  `link_id` int(11) NOT NULL,
  `created_at` datetime NOT NULL,
  PRIMARY KEY (`id`),
  KEY `meta_key` (`meta_key`),
  KEY `link_id` (`link_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Table `lig_prli_link_metas` is empty
--

--
-- Table structure for table `lig_prli_links`
--

CREATE TABLE `lig_prli_links` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `description` text,
  `url` text,
  `slug` varchar(255) DEFAULT NULL,
  `nofollow` tinyint(1) DEFAULT '0',
  `track_me` tinyint(1) DEFAULT '1',
  `param_forwarding` varchar(255) DEFAULT NULL,
  `param_struct` varchar(255) DEFAULT NULL,
  `redirect_type` varchar(255) DEFAULT '307',
  `created_at` datetime NOT NULL,
  `group_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `group_id` (`group_id`),
  KEY `name` (`name`),
  KEY `nofollow` (`nofollow`),
  KEY `track_me` (`track_me`),
  KEY `param_forwarding` (`param_forwarding`),
  KEY `param_struct` (`param_struct`),
  KEY `redirect_type` (`redirect_type`),
  KEY `slug` (`slug`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Table `lig_prli_links` is empty
--

